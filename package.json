		"husky": "^9.1.7",
		"lint-staged": "^16.1.0",
		"playwright": "^1.52.0",
		"prettier": "^3.4.2",
		"prettier-plugin-svelte": "^3.3.3",
		"storybook": "^9.1.0-alpha.2",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"typescript": "^5.0.0",
		"typescript-eslint": "^8.20.0",
		"vite": "^6.2.5",
		"vitest": "^3.1.1",
		"zod": "^3.25.0"
	},
	"dependencies": {
		"@inlang/paraglide-js": "^2.0.0",
		"@thisux/sveltednd": "^0.0.20"
